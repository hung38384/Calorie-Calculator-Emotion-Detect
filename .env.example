# Pls copy this example to .env
# Google Gemini API
NEXT_PUBLIC_GOOGLE_AI_API_KEY="Your API key is here"

JWT_SECRET="JWT_KEY"

# Google Analytics
NEXT_PUBLIC_GA_MEASUREMENT_ID="Your GG_Analystic ID is here"

# Next.js Configuration  
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Image Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=5242880 # 5MB in bytes
NEXT_PUBLIC_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# API Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW_MS=60000 # 1 minute in milliseconds